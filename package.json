{
  "name": "f2",
  "description": "Charts for mobile visualization.",
  "keywords": [
    "f2",
    "charts",
    "mobile",
    "visualization",
    "data visualization",
    "canvas",
    "2d draw",
    "mobile visualization",
    "interactive"
  ],
  "homepage": "https://github.com/antvis/f2",
  "author": "https://github.com/orgs/antvis/people",
  "repository": {
    "type": "git",
    "url": "https://github.com/antvis/f2"
  },
  "bugs": {
    "url": "https://github.com/antvis/f2/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-react-jsx": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "father": "^2.30.0",
    "jest": "^26.6.3",
    "jest-electron": "^0.1.12",
    "jest-less-loader": "^0.1.1",
    "lerna": "^2.11.0",
    "pre-commit": "^1.2.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "clean": "lerna clean",
    "dev": "npm run test-watch",
    "build": "father build",
    "build-watch": "father build -w",
    "test": "jest",
    "test-cov": "jest --coverage",
    "test-live": "npm run test-watch packages",
    "test-watch": "DEBUG_MODE=1 jest --watch",
    "coverage": "jest --coverage",
    "lint": "eslint ./",
    "lint-fix": "eslint --fix ./",
    "publish": "npm run test && npm run build && lerna publish prerelease --dist-tag next",
    "ci": "npm run lint && npm run build && npm run test",
    "refresh": "lerna clean && yarn && lerna bootstrap",
    "site:start": "cd ./packages/site && npm run site:start",
    "site:develop": "cd ./packages/site && npm run site:develop",
    "site:build": "cd ./packages/site && npm run site:build",
    "site:deploy": "cd ./packages/site && npm run site:deploy"
  },
  "pre-commit": {
    "run": [
      "lint",
      "test"
    ],
    "silent": false
  },
  "resolutions": {
    "signal-exit": "3.0.3"
  }
}
